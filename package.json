  "engines": {
    "node": "^16.6.0"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "^13.3.6",
    "@nrwl/eslint-plugin-nx": "^13.3.6",
    "@nrwl/jest": "^13.3.6",
    "@nrwl/linter": "^13.3.6",
    "@nrwl/node": "^13.10.3",
    "@nrwl/nx-cloud": "^13.3.1",
    "@nrwl/tao": "^13.3.6",
    "@nrwl/workspace": "^13.3.6",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "~5.3.1",
    "@typescript-eslint/parser": "~5.3.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.2.3",
    "prettier": "^2.6.2",
    "prettier-eslint": "^8.2.2",
    "ts-jest": "27.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.5.0"
  }
